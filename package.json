{
  "name": "cool-app",
  "productName": "CPU Monitor",
  "version": "1.0.0",
  "description": "My First Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "saili.m",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
